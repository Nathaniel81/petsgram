{
  "expo": {
    "name": "Petsgram",
    "slug": "Petsgram",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_3.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.natexpo.Petsgram"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.natexpo.Petsgram"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "c51b9548-1592-4179-9e80-d0f2f5e7ae6b"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c51b9548-1592-4179-9e80-d0f2f5e7ae6b"
    }
  }
}
